{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [
    {
      "alignment": "left",
      "segments": [
        {
          "style": "plain",
          "template": "{{ .FormattedMs }}",
          "type": "executiontime",
          "properties": {
            "always_enabled": false,
            "style": "austin",
            "threshold": 1000
          }
        },
        {
          "foreground": "lightGreen",
          "style": "plain",
          "template": "{{ .Path }}",
          "type": "path",
          "properties": {
            "style": "full"
          }
        },
        {
          "foreground": "#FFD54F",
          "style": "plain",
          "template": "\u276f ",
          "type": "text"
        }
      ],
      "type": "prompt"
    }
  ],
  "console_title_template": "{{ .Shell }}@{{ .Folder }}",
  "transient_prompt": {
    "background": "transparent",
    "foreground": "#ffffff",
    "template": "❯ ",
    "newline": false
  },
  "secondary_prompt": {
    "template": "❯❯ ",
    "foreground": "#FFD54F",
    "background": "transparent"
  },

  "version": 3
}
